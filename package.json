{
	"name": "ts-tree-ui",
	"description": "Tree UI for the browser, supporting keyboard and drag'n'drop",
	"version": "0.1.0",
	"main": "build/Tree.js",
	"types": "build/Tree.d.ts",
	"license": "MIT",
	"author": "Matthieu Bovel <matthieu@bovel.net>",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/mbovel/ts-tree-ui.git"
	},
	"dependencies": {
		"ts-pubsub": "^0.1.2",
		"ts-tree": "^0.1.6"
	},
	"devDependencies": {
		"@types/node": "^10.7.1",
		"prettier": "^1.14.2",
		"prettier-check": "^2.0.0",
		"tslint": "^5.11.0",
		"typescript": "^3.0.1",
		"webpack": "^4.17.1",
		"webpack-cli": "^3.1.0"
	},
	"scripts": {
		"build-demo": "webpack",
		"build-demo:watch": "webpack --watch",
		"prettier-check": "prettier-check src/*.ts demo/*.ts *.json *.yml",
		"prettier": "prettier --write src/*.ts demo/*.ts *.json *.yml",
		"tslint": "tslint --project tsconfig.json",
		"tslint:fix": "tslint --project tsconfig.json --fix",
		"build": "tsc",
		"preversion": "npm run prettier && npm run tslint:fix && npm run tslint",
		"postversion": "git push && git push --tags"
	}
}
